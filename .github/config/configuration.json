{
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": [
                "feature"
            ]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": [
                "fix"
            ]
        },
        {
            "title": "## 🧪 Tests",
            "labels": [
                "test"
            ]
        },
        {
            "title": "## 💬 Other",
            "labels": [
                "other",
                "dependencies",
                "chore"
            ]
        }
    ],
    "label_extractor": [
        {
            "pattern": "^(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test){1}(\\([\\w\\-\\.]+\\))?(!)?: ([\\w ])+([\\s\\S]*)",
            "on_property": "title",
            "target": "$1"
        }
    ]
}
